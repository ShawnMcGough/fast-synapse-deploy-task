{
  "manifestVersion": 1,
  "id": "fast-synapse-deploy",
  "name": "Fast Synapse Deploy",
  "version": "1.1.16",
  "publisher": "shawn-mcgough",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Deploy Azure Synapse artifacts using the publish branch. A faster alternative to the Microsoft Synapse Deploy task.",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/ShawnMcGough/fast-synapse-deploy-task/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/ShawnMcGough/fast-synapse-deploy-task"
  },
  "files": [
    {
      "path": "FastSynapseDeploy"
    }
  ],
  "contributions": [
    {
      "id": "fast-synapse-deploy",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "FastSynapseDeploy"
      }
    }
  ]
}
